	branches(b) {
		def x = 0;
		if (b)
           	x = x + 7;
       	else
           	x = -7;
		return x;
  }

	loop(b) {
			def c = 1;
			
			while (b < 3) {
				c = c + 1;
				b = b + 1; 
			}
			return c; 
	}

	simple(b) { 
		def x = 10;
		def y = 2;
		def z =1;

	if (b) {
			z = x + y;
		} else {
			z = y - x;
		}
		return z;
	}
}
